<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.openscada.ui.chart"
            name="openSCADA Chart">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.openscada.ui.chart"
            name="openSCADA Chart">
      </category>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.openscada.ui.chart.model.ChartModel.CurrentTimeController"
            class="org.openscada.ui.chart.viewer.controller.time.AdapterFactory">
         <adapter
               type="org.openscada.ui.chart.viewer.controller.ChartControllerFactory">
         </adapter>
      </factory>
   </extension>

</plugin>
